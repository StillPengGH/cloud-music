.container{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #f5f5f5;
}

.header{
  display: flex;
  align-items: center;
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  padding: 10rpx 10rpx;
  position: fixed;
}

.find-box{
  width: 70rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
}

.icon-public{
  font-size: 50rpx;
  color:#333;
}

.search-box{
  flex: 1;
}